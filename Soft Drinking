#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, k, l, c, d, p, nl, np;
    cin >> n >> k >> l >> c >> d >> p >> nl >> np;

    int total_drink = k * l;
    int drink_toasts = total_drink / nl;
    int lime_toasts = c * d;
    int salt_toasts = p / np;

    int ans = min(drink_toasts, min(lime_toasts, salt_toasts)) / n;

    cout << ans;
}
